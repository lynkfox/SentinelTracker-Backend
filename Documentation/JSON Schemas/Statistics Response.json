            },
            "With": {
                "description": "Ally character being compared with",
                "type": "string"
            }
        },
        "required": [
            "With"
        ]
    },
    "InStatistics": {
        "title": "InStatistics",
        "description": "Statistics for a given Environment the game is played IN with the requested Characters (included in any combination of Heroes or Villains that dont include an IN keyword)",

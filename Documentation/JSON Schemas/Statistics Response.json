{
    "title": "StatisticsResponse",
    "type": "object",
    "properties": {
        "RequestedSet": {
            "$ref": "#/definitions/RequestedSet"
        },
        "OriginalRequestedPath": {
            "description": "Original instruction path for the RequestedSet as reference",
            "type": "string"
        },
        "Statistics": {
            "anyOf": [
                {
                    "$ref": "#/definitions/HeroStatistics"
                },
                {
                    "$ref": "#/definitions/OpponentStatistics"
                },
                {
                    "$ref": "#/definitions/Statistics"
                }
            ]
        },
        "Versus": {
            "description": "Links to characters versus this team (if Versus not in RequestedSet)",
            "type": "object"
        },
        "With": {
            "description": "Links to characters with this team (if With not in RequestedSet)",
            "type": "object"
        },
        "In": {
            "description": "Links to environments this set up has played in (if In not in RequestedSet)",
            "type": "object"
        }
    },
    "required": [
        "RequestedSet",
        "OriginalRequestedPath"
    ],
    "definitions": {
        "RequestedSet": {
            "title": "RequestedSet",
            "description": "Details about the requested data set, which heroes, villains, environments, and users were asked to
be retrieved in combination", "type": "object", "properties": {"heroes": {"type": "array", "items": {"type": "string"}}, "villains": {"type": "array", "items": {"type": "string"}}, "environment": {"type": "array", "items": {"type": "string"}},
"user": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            }
        }
    },
    "AgainstStatistics": {
        "title": "AgainstStatistics",
        "description": "Statistics for a given character AGAINST the Requested Character (only included in single side lookups that don\'t include a VERSUS keyword)",
        "type": "object",
        "properties": {
            "TotalGames": {
                "description": "Total games recorded with this RequestedSet",
                "default": 0,
                "type": "integer"
            },
            "TotalWins": {
                "description": "Total wins recorded with this RequestedSet",
                "default": 0,
                "type": "integer"
            },
            "WinRate": {
                "description": "Derived: Win percentage",
                "default": 0.0,
                "type": "number"
            },
            "TotalLosses": {
                "description": "Derived: Number of losses",
                "default": 0,
                "type": "integer"
            },
            "LossRate": {
                "description": "Derived: Loss percentage",
                "default": 0.0,
                "type": "number"
            },
            "Against": {
                "description": "Opponent character (Hero or Villain) being compared with",
                "type": "string"
            }
        },
        "required": [
            "Against"
        ]
    },
    "WithStatistics": {
        "title": "WithStatistics",
        "description": "Statistics for a given character WITH (as an ally of) the Requested Character (only included in single character lookups that dont already include a WITH keyword)",
        "type": "object",
